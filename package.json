{
  "name": "backend",
  "version": "1.0.0",
  "main": ".dist/server.js",
  "repository": "git@github.com:Eas-Eat/Backend.git",
  "author": "Dylan DE SOUSA <dds1991@hotmail.fr>",
  "license": "MIT",
  "scripts": {
    "test:static": "tsc",
    "test:unit": "yarn jest --config=.jestrc.js",
    "test": "yarn test:static && yarn test:unit",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "yarn run prisma generate",
    "generate:nexus": "babel-node --extensions \".ts\" src/schema",
    "db:migrate": "yarn prisma migrate up --experimental",
    "clean": "yarn rimraf .dist",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "yarn prettier --write \"**/*.ts\" --loglevel silent",
    "build": "yarn clean && yarn babel src/ --out-dir .dist/ --extensions \".ts,.tsx\" --source-maps inline --quiet --copy-files",
    "dev": "yarn nodemon",
    "start": "node .dist/server.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@prisma/cli": "^2.0.0-beta.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.0",
    "@types/node-mailjet": "^3.3.3",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "apollo-server-testing": "^2.12.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "@nexus/schema": "^0.13.1",
    "@prisma/client": "^2.0.0-beta.2",
    "apollo-server": "^2.12.0",
    "dotenv": "^8.2.0",
    "nexus-prisma": "^0.12.0",
    "node-mailjet": "^3.3.1"
  }
}
