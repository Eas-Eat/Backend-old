// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String @id @default(uuid())
  email    String @unique
  password String
  name     String

  @@index([id, email])
}

model Cuisine {
  type     String @id
  svgName  String
}

model FoodInventory {
  id       Int    @id @default(autoincrement())
  userId   String
  foodId   String

  @@index([userId, foodId])
  @@unique([userId, foodId])
}
